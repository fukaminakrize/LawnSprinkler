<div id="sidebar" class="sidebar-offcanvas">
	<div class="col-md-12">
		<ul class="nav nav-tabs nav-stacked nav-sidebar">
			<% subsections.forEach(function(subsection) { %>
				<li class="<%= ((subsections.indexOf(subsection) == 0) ? 'active' : '')%>"><a data-toggle="tab" href="#<%= subsection.view %>"><%= subsection.name %></a></li>
			<% }); %>
		</ul>
	</div>
</div>